{
	"name": "Gruvbox Light Hard Black Theme",
	"dark": false,
	"author": "Adam Wojszczyk",
	"editorScheme": "/intellij.scheme.xml",
	"ui": {
		"*": {
			"background": "$ui.background",
			"foreground": "$ui.foreground",
			"infoForeground": "$ui.infoForeground",
			"lightSelectionBackground": "$ui.lightSelectionBackground",
			"selectionBackground": "$ui.selectionBackground",
			"selectionForeground": "$ui.selectionForeground",
			"selectionBackgroundInactive": "$ui.selectionBackgroundInactive",
			"selectionInactiveBackground": "$ui.selectionInactiveBackground",
			"selectedInactiveBackground": "$ui.selectedInactiveBackground",
			"selectedBackgroundInactive": "$ui.selectedBackgroundInactive",
			"selectedBackground": "$ui.selectedBackground",
			"selectedForeground": "$ui.selectedForeground",
			"hoverBackground": "$ui.hoverBackground",
			"borderColor": "$ui.borderColor",
			"disabledBorderColor": "$ui.disabledBorderColor",
			"separatorColor": "$ui.separatorColor"
		},
		"ActionButton": {
			"hoverBackground": "$ui.actionButton.hoverBackground"
		},
		"Button": {
			"startBackground": "$ui.button.startBackground",
			"endBackground": "$ui.button.endBackground",
			"startBorderColor": "$ui.button.startBorderColor",
			"endBorderColor": "$ui.button.endBorderColor",
			"default": {
				"foreground": "$ui.button.defaultt.foreground",
				"startBackground": "$ui.button.defaultt.startBackground",
				"endBackground": "$ui.button.defaultt.endBackground",
				"startBorderColor": "$ui.button.defaultt.startBorderColor",
				"endBorderColor": "$ui.button.defaultt.endBorderColor",
				"focusedBorderColor": "$ui.button.defaultt.focusedBorderColor"
			}
		},
		"ComboBox": {
			"nonEditableBackground": "$ui.comboBox.nonEditableBackground",
			"ArrowButton": {
				"iconColor": "$ui.comboBox.arrowButton.iconColor",
				"disabledIconColor": "$ui.comboBox.arrowButton.disabledIconColor",
				"nonEditableBackground": "$ui.comboBox.arrowButton.nonEditableBackground"
			}
		},
		"EditorTabs": {
			"underlinedTabBackground": "$ui.editorTabs.underlinedTabBackground",
			"underlineColor": "$ui.editorTabs.underlineColor"
		},
		"ToolWindow": {
			"Header": {
				"background": "$ui.toolWindow.header.background",
				"inactiveBackground": "$ui.toolWindow.header.inactiveBackground"
			},
			"HeaderTab": {
				"selectedInactiveBackground": "$ui.toolWindow.headerTab.selectedInactiveBackground",
				"hoverInactiveBackground": "$ui.toolWindow.headerTab.hoverInactiveBackground"
			}
		},
		"Table": {
			"stripeColor": "$ui.table.stripeColor",
			"lightSelectionForeground": "$ui.table.lightSelectionForeground",
			"lightSelectionInactiveForeground": "$ui.table.lightSelectionInactiveForeground",
			"lightSelectionBackground": "$ui.table.lightSelectionBackground",
			"lightSelectionInactiveBackground": "$ui.table.lightSelectionInactiveBackground"
		},
		"FileColor": {
			"Yellow": "$ui.fileColor.yellow",
			"Green": "$ui.fileColor.green",
			"Blue": "$ui.fileColor.blue",
			"Violet": "$ui.fileColor.violet",
			"Orange": "$ui.fileColor.orange",
			"Rose": "$ui.fileColor.rose"
		},
		"Link": {
			"activeForeground": "$ui.link.activeForeground",
			"hoverForeground": "$ui.link.hoverForeground",
			"pressedForeground": "$ui.link.pressedForeground",
			"visitedForeground": "$ui.link.visitedForeground"
		}
	},
	"icons": {
		"ColorPalette": {
			"Actions.Grey": "$ui.icons.colorPalette.actions.grey",
			"Actions.Red": "$ui.icons.colorPalette.actions.red",
			"Actions.Yellow": "$ui.icons.colorPalette.actions.yellow",
			"Actions.Green": "$ui.icons.colorPalette.actions.green",
			"Actions.Blue": "$ui.icons.colorPalette.actions.blue",
			"Actions.GreyInline.Dark": "$ui.icons.colorPalette.actions.greyInline.dark",
			"Objects.Grey": "$ui.icons.colorPalette.objects.grey",
			"Objects.RedStatus": "$ui.icons.colorPalette.objects.redStatus",
			"Objects.Red": "$ui.icons.colorPalette.objects.red",
			"Objects.Pink": "$ui.icons.colorPalette.objects.pink",
			"Objects.Yellow": "$ui.icons.colorPalette.objects.yellow",
			"Objects.Green": "$ui.icons.colorPalette.objects.green",
			"Objects.Blue": "$ui.icons.colorPalette.objects.blue",
			"Objects.Purple": "$ui.icons.colorPalette.objects.purple",
			"Objects.BlackText": "$ui.icons.colorPalette.objects.blackText",
			"Objects.YellowDark": "$ui.icons.colorPalette.objects.yellowDark",
			"Objects.GreenAndroid": "$ui.icons.colorPalette.objects.greenAndroid",
			"Checkbox.Foreground.Selected.Dark": "$ui.icons.colorPalette.checkboxes.foreground.selected",
			"Checkbox.Foreground.Disabled.Dark": "$ui.icons.colorPalette.checkboxes.foreground.disabled",
			"Checkbox.Background.Default.Dark": "$ui.icons.colorPalette.checkboxes.background.defaultt",
			"Checkbox.Background.Disabled.Dark": "$ui.icons.colorPalette.checkboxes.background.disabled",
			"Checkbox.Border.Default.Dark": "$ui.icons.colorPalette.checkboxes.border.defaultt",
			"Checkbox.Border.Disabled.Dark": "$ui.icons.colorPalette.checkboxes.border.disabled",
			"Checkbox.Focus.Wide.Dark": "$ui.icons.colorPalette.checkboxes.focus.wide",
			"Checkbox.Focus.Thin.Default.Dark": "$ui.icons.colorPalette.checkboxes.focus.thin.defaultt",
			"Checkbox.Focus.Thin.Selected.Dark": "$ui.icons.colorPalette.checkboxes.focus.thin.selected"
		}
	}
}
